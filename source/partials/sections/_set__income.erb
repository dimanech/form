	<table class="table">
		<colgroup>
			<col />
			<col />
			<col width="200" />
			<col width="200" />
		</colgroup>
		<tr>
			<td class="table__cell_bd-r table__psdo-th">
				8.
			</td>
			<th class="table__cell_fwn">
